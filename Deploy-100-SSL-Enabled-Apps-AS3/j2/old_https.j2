{
  "class": "AS3",
  "action": "deploy",
  "persist": true,
  "declaration": {
    "class": "ADC",
    "schemaVersion": "3.0.0",
    "controls": {
      "logLevel": "debug",
      "dryRun": false
    },
    "id": "usecase1",
    "label": "Ansible Workshops",
    "remark": "HTTPS with pool",
    "App-Tennant-1": {
      "class": "Tenant",
      "AS3-UseCase-1": {
        "class": "Application",
        "serviceMain": {
          "class": "Service_HTTPS",
          "virtualAddresses": [
            "199.199.199.199"
          ],
          "profileMultiplex": {
              "bigip": "/Common/oneconnect"
          },
          "pool": "serviceMain_pool",
#          "serverTLS": "serviceMain_cert"
          "serverTLS": {
					    "bigip": "/Common/clientssl"
					},
          "persistenceMethods": []
        },
        "serviceMain_pool": {
          "class": "Pool",
					"minimumMembersActive": 0,
					"minimumMonitors": "all",
          "monitors": [
            "http"
          ],
          "members": [{
            "servicePort": 80,
            "serverAddresses": ["192.168.199.199"]
          }]
        }
      }
    }
  }
}
